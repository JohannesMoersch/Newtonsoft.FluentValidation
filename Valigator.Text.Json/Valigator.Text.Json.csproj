<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Title>Valigator.Text.Json</Title>
		<Description>This package adds support for serializing and deserializing valigator types to System.Text.Json.</Description>
		<PackageProjectUrl>https://github.com/JohannesMoersch/Valigator</PackageProjectUrl>
		<Authors>Johannes Moersch</Authors>
		<PackageTags>validation</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Copyright>Copyright Â© 2019 Johannes Moersch</Copyright>
		<Version>1.11.3</Version>
		<LangVersion>Latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Functional.Primitives" Version="1.10.0" />
    <PackageReference Include="System.Text.Json" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Valigator.Core\Valigator.Core.csproj" />
  </ItemGroup>

</Project>
